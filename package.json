{
  "name": "lireddit-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "gen-env": "gen-env-types .env -o src/env.d.ts -e .",
    "build": "tsc",
    "watch": "tsc -w",
    "dev": "nodemon dist/index.js",
    "start": "node dist/index.js",
    "start2": "ts-node src/index.ts",
    "dev2": "nodemon --exec ts-node src/index.ts",
    "migration:generate": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:generate -d src/ormconfig.ts",
    "migration:up": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run -d src/ormconfig.ts",
    "migration:down": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert -d src/ormconfig.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/connect-redis": "^0.0.14",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.1",
    "@types/node": "^17.0.21",
    "@types/nodemailer": "^6.4.4",
    "@types/redis": "^4.0.11",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.1",
    "class-validator": "^0.13.2",
    "nodemon": "^2.0.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "apollo-server-express": "^3.6.4",
    "argon2": "^0.28.5",
    "connect-redis": "^6.1.3",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "gen-env-types": "^1.3.4",
    "graphql": "^15.3.0",
    "graphql-codegen": "^0.4.0",
    "init": "^0.1.2",
    "ioredis": "^4.28.5",
    "nodemailer": "^6.7.3",
    "pg": "^8.7.3",
    "redis": "^4.0.4",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.3.3",
    "uuid": "^8.3.2"
  }
}
